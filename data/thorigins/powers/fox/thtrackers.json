{
	"type": "origins:multiple",
	"hidden": true,

	"first_time_chosen": {
		"type": "origins:toggle",
		"active_by_default": false,
		"key": "nuh.uh"
	},

	"optional_powers": {
		"type": "origins:action_on_callback",
		"entity_action_added": {
			"type": "origins:delay",
			"ticks": 1,
			"action": {
				"type": "origins:and",
				"actions": [
					{
						"type": "origins:if_else",
						"condition": {
							"type": "origins:origin",
							"layer": "thorigins:foxsize",
							"origin": "origins:empty"
						},
						"if_action": {
							"type": "origins:execute_command",
							"command": "/origin gui @s thorigins:foxsize"
						}
					},
					{
						"type": "origins:if_else",
						"condition": {
							"type": "origins:resource",
							"resource": "*:fox/universal_settings_optional_walk_on_powdered_snow",
							"compare_to": 1,
							"comparison": "=="
						},
						"if_action": {
							"type": "origins:grant_power",
							"power": "*:optional_power/thpowdersnow",
							"source": "thorigins:universal"
						},
						"else_action": {
							"type": "origins:revoke_power",
							"power": "*:optional_power/thpowdersnow",
							"source": "thorigins:universal"
						}
					},
					{
						"type": "origins:if_else",
						"condition": {
							"type": "origins:resource",
							"resource": "*:fox/universal_settings_optional_mute_fallsound",
							"compare_to": 1,
							"comparison": "=="
						},
						"if_action": {
							"type": "origins:grant_power",
							"power": "*:optional_power/thfallsound",
							"source": "thorigins:universal"
						},
						"else_action": {
							"type": "origins:revoke_power",
							"power": "*:optional_power/thfallsound",
							"source": "thorigins:universal"
						}
					}
				]
			}
		},
		"entity_action_lost": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:revoke_power",
					"power": "*:optional_power/thpowdersnow",
					"source": "thorigins:universal"
				},
				{
					"type": "origins:revoke_power",
					"power": "*:optional_power/thfallsound",
					"source": "thorigins:universal"
				},
				{
					"type": "origins:execute_command",
					"command": "/origin set @s thorigins:foxsize origins:empty"
				}
			]
		}
	},

	"scale_modified": {
		"type": "origins:resource",
		"start_value": 0,
		"min": 0,
		"max": 2,
		"max_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:execute_command",
					"command": "/scale persist set true @s"
				},
				{
					"type": "origins:execute_command",
					"command": "/scale multiply pehkui:drops 0.7 @s"
				},
				{
					"type": "origins:execute_command",
					"command": "/scale multiply pehkui:visibility 0.8 @s"
				},
				{
					"type": "origins:execute_command",
					"command": "/scale easing set pehkui:height pehkui:exponential_out @s"
				},
				{
					"type": "origins:change_resource",
					"resource": "*:*_scale_modified",
					"change": 1,
					"operation": "set"
				}
			]
		}
	},

	"origin_tracker": {
		"type": "origins:resource",
		"min": 0,
		"max": 5,
		"start_value": 0
	},

	"ticks_20": {
		"type": "origins:action_over_time",
		"interval": 20,
		"entity_action": {
			"type": "origins:if_else",
			"condition": {
				"type": "origins:and",
				"conditions": [
					{
						"type": "origins:command",
						"command": "/origin has @s origins:origin thorigins:element_fox",
						"compare_to": 0,
						"comparison": "=="
					},
					{
						"type": "origins:command",
						"command": "/origin has @s origins:origin thorigins:fox",
						"compare_to": 0,
						"comparison": "=="
					}
				]
			},
			"if_action": {
				"type": "origins:and",
				"actions": [
					{
						"type": "origins:if_else",
						"condition": {
							"type": "origins:nbt",
							"nbt": "{ \"Tags\": [\"foxie\"] }"
						},
						"if_action": {
							"type": "origins:execute_command",
							"command": "/tag @s remove foxie"
						}
					},
					{
						"type": "origins:if_else",
						"condition": {
							"type": "origins:resource",
							"resource": "*:*_scale_modified",
							"compare_to": 1,
							"comparison": "=="
						},
						"if_action": {
							"type": "origins:and",
							"actions": [
								{
									"type": "origins:change_resource",
									"resource": "*:*_scale_modified",
									"change": 0,
									"operation": "set"
								},
								{
									"type": "origins:execute_command",
									"command": "/scale reset @s"
								}
							]
						}
					},
					{
						"type": "origins:execute_command",
						"command": "/origin set @s thorigins:foxsize origins:empty"
					}
				]
			},
			"else_action": {
				"type": "origins:if_else",
				"condition": {
					"type": "origins:nbt",
					"nbt": "{ \"Tags\": [\"foxie\"] }",
					"inverted": true
				},
				"if_action": {
					"type": "origins:execute_command",
					"command": "/tag @s add foxie"
				}
			}
		}
	},

	"warden_kills": {
