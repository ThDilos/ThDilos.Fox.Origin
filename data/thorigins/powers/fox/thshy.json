{
	"type": "origins:multiple",
	"badges": [
		{
			"type": "origins:keybind",
			"sprite": "minecraft:textures/item/egg.png",
			"text": "origin.thorigin.passive"
		},
		{
			"type": "origins:keybind",
			"sprite": "origins:textures/gui/badge/info.png",
			"text": "thshy.info"
		}
	],

	"fear": {
		"type": "origins:resource",
		"hud_render": {
			"should_render": false
		},
		"max": 5,
		"min": 0,
		"start_value": 0,
		"max_action": {
			"type": "origins:change_resource",
			"resource": "*:*_feartimer",
			"change": 100,
			"operation": "set"
		}
	},

	"feartimer": {
		"type": "origins:resource",
		"min": 0,
		"max": 100,
		"hud_render": {
			"should_render": false
		}
	},

	"auto_decrement": {
		"type": "origins:action_over_time",
		"interval": 1,
		"entity_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:change_resource",
					"resource": "*:*_feartimer",
					"change": -1
				},
				{
					"type": "origins:change_resource",
					"resource": "*:*_shakingtimer",
					"change": -1
				}
			]
		}
	},

	"fear_rectifier": {
		"type": "origins:action_over_time",
		"interval": 2,
		"condition": {
			"type": "origins:resource",
			"resource": "*:*_feartimer",
			"compare_to": 0,
			"comparison": "=="
		},
		"entity_action": {
			"type": "origins:change_resource",
			"resource": "*:*_fear",
			"change": -1
		}
	},

	"shakingoffear": {
		"type": "origins:shaking",
		"condition": {
			"type": "origins:resource",
			"resource": "*:*_shakingtimer",
			"comparison": ">",
			"compare_to": 0
		}
	},

	"shakingtimer": {
		"type": "origins:resource",
		"min": 0,
		"max": 160,
		"hud_render": {
			"should_render": false
		}
	},

