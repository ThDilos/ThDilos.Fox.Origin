		}
	},

	"ticks": {
		"type": "origins:action_over_time",
		"interval": 1,
		"entity_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:and",
						"conditions": [
							{
								"type": "origins:riding",
								"inverted": true
							},
							{
								"type": "origins:passenger",
								"compare_to": 0,
								"comparison": ">"
							}
						]
					},
					"if_action": {
						"type": "origins:passenger_action",
						"action": {
							"type": "origins:dismount"
						}
					}
				},
				{
					"type": "origins:execute_command",
					"command": "/execute unless entity @p[tag=foxie,limit=1,sort=nearest,distance=..2] run tag @s add disposed"
				},
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:or",
						"conditions": [
							{
								"type": "origins:and",
								"conditions": [
									{
										"type": "origins:riding"
									},
									{
										"type": "origins:passenger",
										"compare_to": 0,
										"comparison": "=="
									}
								]
							},
							{
								"type": "origins:command",
								"command": "/resource get @p[tag=foxie,limit=1,sort=nearest,distance=..2] thorigins:fox/thfoxify_afk_timer",
								"compare_to": 0,
								"comparison": ">"
							}
						]
					},
					"if_action": {
						"type": "origins:execute_command",
						"command": "/tag @s add disposed"
					}
				},
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:command",
						"command": "/execute if entity @s[tag=disposed,tag=sleepin,nbt={NoAI:1b,NoGravity:1b},type=fox]",
						"compare_to": 1,
						"comparison": "=="
					},
					"if_action": {
						"type": "origins:and",
						"actions": [
							{
								"type": "origins:if_else",
								"condition": {
									"type": "origins:riding"
								},
								"if_action": {
									"type": "origins:and",
									"actions": [
										{
											"type": "origins:riding_action",
											"action": {
												"type": "origins:execute_command",
												"command": "/tag @s add to_be_ridden"
											}
										},
										{
											"type": "origins:passenger_action",
											"action": {
												"type": "origins:area_of_effect",
												"radius": 3,
												"bientity_condition": {
													"type": "origins:target_condition",
													"condition": {
														"type": "origins:nbt",
														"nbt": "{\"Tags\":[\"to_be_ridden\"]}"
													}
												},
												"bientity_action": {
													"type": "origins:mount"
												}
											}
										},
										{
											"type": "origins:riding_action",
											"action": {
												"type": "origins:execute_command",
												"command": "/tag @s remove to_be_ridden"
											}
										},
										{
											"type": "origins:dismount"
										}
									]
								}
							},
							{
								"type": "origins:if_else",
								"condition": {
									"type": "origins:passenger",
									"compare_to": 0,
									"comparison": "=="
								},
								"if_action": {
									"type": "origins:and",
									"actions": [
										{
											"type": "origins:execute_command",
											"command": "/tp @s[tag=disposed,tag=sleepin,nbt={NoAI:1b,NoGravity:1b},type=fox] ~ ~500 ~"
										},
										{
											"type": "origins:delay",
											"ticks": 5,
											"action": {
												"type": "origins:execute_command",
												"command": "/kill @s[tag=disposed,tag=sleepin,nbt={NoAI:1b,NoGravity:1b},type=fox]"
											}
										}
									]
								}
							}
						]
					}
				}
			]
		}
	}
