	"exit": {
		"type": "origins:resource",
		"min": 0,
		"max": 1,
		"start_value": 0,
		"max_action": [
			{
				"type": "origins:execute_command",
				"command": "/data remove entity @s HandItems[0]"
			},
			{
				"type": "origins:if_else",
				"condition": {
					"type": "origins:riding"
				},
				"if_action": {
					"type": "origins:and",
					"actions": [
						{
							"type": "origins:riding_action",
							"action": {
								"type": "origins:execute_command",
								"command": "/tag @s add to_be_ridden"
							}
						},
						{
							"type": "origins:passenger_action",
							"bientity_action": {
								"type": "origins:target_action",
								"action": {
									"type": "origins:area_of_effect",
									"radius": 3,
									"bientity_condition": {
										"type": "origins:target_condition",
										"condition": {
											"type": "origins:nbt",
											"nbt": "{\"Tags\":[\"to_be_ridden\"]}"
										}
									},
									"bientity_action": {
										"type": "origins:mount"
									}
								}
							}
						},
						{
							"type": "origins:riding_action",
							"action": {
								"type": "origins:execute_command",
								"command": "/tag @s remove to_be_ridden"
							}
						},
						{
							"type": "origins:dismount"
						}
					]
				}
			},
			{
				"type": "origins:delay",
				"ticks": 1,
				"action": {
					"type": "origins:and",
					"actions": [
						{
							"type": "origins:execute_command",
							"command": "/data remove entity @s HandItems[0]"
						},
						{
							"type": "origins:passenger_action",
							"bientity_action": {
								"type": "origins:target_action",
								"action": {
									"type": "origins:dismount"
								}
							}
						},
						{
							"type": "origins:delay",
							"ticks": 1,
							"action": {
								"type": "origins:execute_command",
								"command": "/tp @s ~ ~-500 ~"
							}
						},
						{
							"type": "origins:execute_command",
							"command": "/data merge entity @s {Silent:1b}"
						}
					]
				}
			},
			{
				"type": "origins:delay",
				"ticks": 5,
				"action": {
					"type": "origins:and",
					"actions": [
						{
							"type": "origins:execute_command",
							"command": "/data remove entity @s HandItems[0]"
						},
						{
							"type": "origins:execute_command",
							"command": "/kill @s"
						}
					]
				}
			}
		]
