	"ticks": {
		"type": "origins:action_over_time",
		"interval": 2,
		"entity_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:and",
						"conditions": [
							{
								"type": "origins:or",
								"conditions": [
									{
										"type": "origins:resource",
										"resource": "*:fox/thhunt_prepare_stage",
										"compare_to": 25,
										"comparison": ">"
									},
									{
										"type": "origins:power",
										"power": "*:fox/thhunt",
										"inverted": true
									}
								]
							},
							{
								"type": "origins:gamemode",
								"gamemode": "survival"
							},
							{
								"type": "origins:resource",
								"resource": "*:fox/thfoxify_afk_timer",
								"compare_to": 1,
								"comparison": ">"
							}
						]
					},
					"if_action": {
						"type": "origins:and",
						"actions": [
							{
								"type": "origins:execute_command",
								"command": "/execute as @e[type=wolf,distance=..18] unless data entity @s Owner run data modify entity @s AngerTime set value 4"
							},
							{
								"type": "origins:execute_command",
								"command": "/execute as @e[type=polar_bear,distance=..16] unless data entity @s Owner run data modify entity @s AngerTime set value 4"
							},
							{
								"type": "origins:delay",
								"ticks": 1,
								"action": {
									"type": "origins:and",
									"actions": [
										{
											"type": "origins:execute_command",
											"command": "/execute as @e[type=wolf,distance=..18] unless data entity @s Owner run data modify entity @s AngryAt set from entity @p[tag=foxie] UUID"
										},
										{
											"type": "origins:execute_command",
											"command": "/execute as @e[type=polar_bear,distance=..16] unless data entity @s Owner run data modify entity @s AngryAt set from entity @p[tag=foxie] UUID"
										}
									]
								}
							}
						]
					}
				},
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:command",
						"command": "/execute if entity @e[type=minecraft:fox,distance=..15,tag=!sleepin,tag=!hatting]",
						"compare_to": 0,
						"comparison": ">"
					},
					"if_action": {
						"type": "origins:execute_command",
						"command": "/execute as @e[type=minecraft:fox,distance=3..15,tag=!sleepin,tag=!hatting] run data modify entity @s Trusted append from entity @p[tag=foxie] UUID"
					}
				}
			]
		}
	},

