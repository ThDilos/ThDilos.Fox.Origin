			"comparison": "==",
			"compare_to": 0
		}
	},

	"reset_le_switch": {
		"type": "origins:action_over_time",
		"interval": 1,
		"entity_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:and",
						"conditions": [
							{
								"type": "origins:power_active",
								"power": "*:*_le_switch"
							},
							{
								"type": "origins:sneaking",
								"inverted": true
							}
						]
					},
					"if_action": {
						"type": "origins:toggle",
						"power": "*:*_le_switch"
					}
				},
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:power_active",
						"power": "*:*_le_switch",
						"inverted": true
					},
					"if_action": {
						"type": "origins:change_resource",
						"resource": "*:*_timer",
						"change": 50,
						"operation": "set"
					}
				}
			]
		}
	},

