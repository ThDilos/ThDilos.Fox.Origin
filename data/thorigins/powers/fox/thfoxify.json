						"change": 5,
						"operation": "set"
					}
				},
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:resource",
						"resource": "*:*_afk_timer",
						"compare_to": 0,
						"comparison": "=="
					},
					"if_action": {
						"type": "origins:execute_command",
						"command": "/tp @e[type=fox,nbt={Sleeping:1b,NoAI:1b,NoGravity:1b},tag=sleepin,distance=..0.2,limit=1] ~ ~ ~ facing ^-0.3 ^ ^-0.15"
					}
				}
			]
		}
	},

