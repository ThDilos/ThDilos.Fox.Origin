	"fix_bad_fox": {
		"type": "origins:action_over_time",
		"interval": 20,
		"entity_action": {
			"type": "origins:execute_command",
			"command": "/execute as @e[type=fox,nbt={Sleeping:1b,NoAI:1b,NoGravity:1b},tag=sleepin] run execute unless entity @p[distance=..0.3] run kill @s"
		}
	},

	"auto_decrement": {
		"type": "origins:action_over_time",
		"interval": 1,
		"entity_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:resource",
						"resource": "*:*_afk_timer",
						"compare_to": 0,
						"comparison": ">"
					},
					"if_action": {
						"type": "origins:change_resource",
						"resource": "*:*_afk_timer",
						"change": -1
					}
				},
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:and",
						"conditions": [
							{
								"type": "origins:resource",
								"resource": "*:*_pressed",
								"compare_to": 0,
								"comparison": "=="
							},
							{
								"type": "origins:resource",
								"resource": "*:*_mounting",
								"compare_to": 0,
								"comparison": ">"
							}
						]
					},
					"if_action": {
						"type": "origins:change_resource",
						"resource": "*:*_mounting",
						"change": 0,
						"operation": "set"
					}
				},
				{
					"type": "origins:change_resource",
					"resource": "*:*_pressed",
					"change": -1
				}
			]
		}
	},

