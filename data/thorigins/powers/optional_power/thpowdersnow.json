										"type": "origins:block",
										"block": "minecraft:powder_snow"
									},
									"offset_y": -0.8
								},
								{
									"type": "origins:resource",
									"resource": "*:fox/thfoxify_figura_state",
									"compare_to": 1,
									"comparison": "=="
								}
							]
						}
					]
				},
				{
					"type": "origins:block_collision",
					"block_condition": {
						"type": "origins:block",
						"block": "minecraft:powder_snow"
					},
					"inverted": true
				},
				{
					"type": "origins:resource",
					"resource": "*:*_sink_in",
					"compare_to": 0,
					"comparison": "=="
				},
				{
					"type": "origins:equipped_item",
					"inverted": true,
					"equipment_slot": "feet",
					"item_condition": {
						"type": "origins:ingredient",
						"ingredient": {
							"item": "minecraft:leather_boots"
						}
					}
				}
			]
		}
	},

