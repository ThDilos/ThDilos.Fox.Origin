{
	"type": "origins:multiple",

	"_init_": {
		"type": "origins:action_on_callback",
		"entity_action_added": {
			"type": "origins:execute_command",
			"command": "/scoreboard objectives add yIntPos dummy \"yIntPos\""
		}
	},

	"was_air": {
		"type": "origins:resource",
		"min": 0,
		"max": 1,
		"start_value": 1,
		"min_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:execute_command",
					"command": "/summon marker ~ ~ ~ {Tags:[\"anchor\"]}"
				},
				{
					"type": "origins:execute_command",
					"command": "/execute store result score @s yIntPos run data get entity @s Pos[1]"
				},
				{
					"type": "origins:execute_command",
					"command": "/execute store result entity @e[type=marker,tag=anchor,limit=1,sort=nearest] Pos[1] double 1 run scoreboard players get @s yIntPos"
				},
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:block_collision",
						"block_condition": {
							"type": "origins:block",
							"block": "minecraft:powder_snow"
						}
					},
					"if_action": {
						"type": "origins:execute_command",
						"command": "/execute at @e[type=marker,tag=anchor,limit=1,sort=nearest] run tp @s ~ ~1 ~"
					},
					"else_action": {
						"type": "origins:execute_command",
						"command": "/execute at @e[type=marker,tag=anchor,limit=1,sort=nearest] run tp @s ~ ~ ~"
					}
				},
				{
					"type": "origins:execute_command",
					"command": "/kill @e[type=marker,tag=anchor,limit=1,sort=nearest]"
				}
			]
		}
	},

	"sink_in": {
		"type": "origins:cooldown",
		"cooldown": 1
	},

	"pounce_effect": {
		"type": "origins:active_self",
		"cooldown": 1,
		"key": "key.origins.primary_active",
		"entity_action": {
			"type": "origins:trigger_cooldown",
			"power": "*:*_jumping"
		}
	},

	"jump_detection": {
		"type": "origins:active_self",
		"key": {
			"key": "key.jump",
			"continuous": true
		},
		"entity_action": {
			"type": "origins:if_else",
			"condition": {
				"type": "origins:sneaking"
			},
			"if_action": {
				"type": "origins:if_else",
				"condition": {
					"type": "origins:block_collision",
					"inverted": true,
					"block_condition": {
						"type": "origins:block",
						"block": "minecraft:powder_snow"
					}
				},
				"if_action": {
					"type": "origins:trigger_cooldown",
					"power": "*:*_sink_in"
				}
			},
			"else_action": {
				"type": "origins:trigger_cooldown",
				"power": "*:*_jumping"
			}
		}
	},

	"jumping": {
		"type": "origins:cooldown",
		"cooldown": 1
	},

	"ticks": {
		"type": "origins:action_over_time",
		"interval": 1,
		"entity_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:power_active",
						"power": "*:*_stop_fall"
					},
					"if_action": {
						"type": "origins:change_resource",
						"resource": "*:*_was_air",
						"change": -1
					}
				},
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:block_collision",
						"inverted": true,
						"block_condition": {
							"type": "origins:block",
							"block": "minecraft:powder_snow"
						},
						"offset_y": -0.01
					},
					"if_action": {
						"type": "origins:change_resource",
						"resource": "*:*_was_air",
						"change": 1
					}
				}
			]
		}
	},

	"grounded_for_jump": {
		"type": "origins:grounded",
		"condition": {
			"type": "origins:all_of",
			"conditions": [
				{
