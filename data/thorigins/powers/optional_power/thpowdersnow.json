{
	"type": "origins:multiple",

	"_init_": {
		"type": "origins:action_on_callback",
		"entity_action_added": {
			"type": "origins:delay",
			"ticks": 1,
			"action": {
				"type": "origins:if_else",
				"condition": {
					"type": "origins:command",
					"command": "/execute as @s[nbt={ForgeCaps:{}}] run list",
					"compare_to": 1,
					"comparison": ">"
				},
				"if_action": {
					"type": "origins:and",
					"actions": [
						{
							"type": "origins:change_resource",
							"resource": "*:fox/universal_settings_optional_walk_on_powdered_snow",
							"change": 0,
							"operation": "set"
						},
						{
							"type": "origins:revoke_power",
							"power": "*:optional_power/thpowdersnow",
							"source": "thorigins:universal"
						},
						{
							"type": "origins:execute_command",
							"command": "/tellraw @s [{\"translate\":\"thorigins.forge.server.detected\"}]"
						}
					]
				}
			}
		}
	},

	"was_air": {
		"type": "origins:resource",
		"min": 0,
		"max": 1,
		"start_value": 1,
		"min_action": {
			"type": "origins:if_else",
			"condition": {
				"type": "origins:block_collision",
				"block_condition": {
					"type": "origins:block",
					"block": "minecraft:powder_snow"
				}
			},
			"if_action": {
				"type": "origins:execute_command",
				"command": "/execute align y run tp @s ~ ~1 ~"
			},
			"else_action": {
				"type": "origins:execute_command",
				"command": "/execute align y run tp @s ~ ~ ~"
			}
		}
	},

	"sink_in": {
		"type": "origins:cooldown",
		"cooldown": 1
	},

	"pounce_effect": {
		"type": "origins:active_self",
		"cooldown": 1,
		"key": "key.origins.primary_active",
		"entity_action": {
			"type": "origins:trigger_cooldown",
			"power": "*:*_jumping"
		}
	},

	"jump_detection": {
		"type": "origins:active_self",
		"key": {
			"key": "key.jump",
			"continuous": true
		},
		"entity_action": {
			"type": "origins:if_else",
			"condition": {
				"type": "origins:sneaking"
			},
			"if_action": {
				"type": "origins:if_else",
				"condition": {
					"type": "origins:block_collision",
					"inverted": true,
					"block_condition": {
						"type": "origins:block",
						"block": "minecraft:powder_snow"
					}
				},
				"if_action": {
					"type": "origins:trigger_cooldown",
					"power": "*:*_sink_in"
				}
			},
			"else_action": {
				"type": "origins:trigger_cooldown",
				"power": "*:*_jumping"
			}
		}
	},

	"jumping": {
		"type": "origins:cooldown",
		"cooldown": 1
	},

	"ticks": {
		"type": "origins:action_over_time",
		"interval": 1,
		"entity_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:power_active",
						"power": "*:*_stop_fall"
					},
					"if_action": {
						"type": "origins:change_resource",
						"resource": "*:*_was_air",
						"change": -1
					}
				},
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:block_collision",
						"inverted": true,
						"block_condition": {
							"type": "origins:block",
							"block": "minecraft:powder_snow"
						},
						"offset_y": -0.01
					},
					"if_action": {
						"type": "origins:change_resource",
						"resource": "*:*_was_air",
						"change": 1
					}
				}
			]
		},
		"condition": {
			"type": "origins:or",
			"conditions": [
				{
					"type": "origins:power_active",
					"power": "*:fox/thweak_heavy_armor",
					"inverted": true
				},
				{
					"type": "origins:power",
					"power": "*:fox/thweak_heavy_armor",
					"inverted": true
				}
			]
		}
	},

	"grounded_for_jump": {
		"type": "origins:grounded",
		"condition": {
			"type": "origins:and",
			"conditions": [
				{
					"type": "origins:block_collision",
					"block_condition": {
						"type": "origins:block",
						"block": "minecraft:powder_snow"
					},
					"offset_y": -0.3
				},
				{
					"type": "origins:block_collision",
					"block_condition": {
						"type": "origins:block",
						"block": "minecraft:powder_snow"
					},
					"inverted": true
				},
				{
					"type": "origins:resource",
					"resource": "*:*_sink_in",
					"compare_to": 0,
					"comparison": "=="
				},
				{
					"type": "origins:equipped_item",
					"inverted": true,
					"equipment_slot": "feet",
					"item_condition": {
						"type": "origins:ingredient",
						"ingredient": {
							"item": "minecraft:leather_boots"
						}
					}
				},
				{
					"type": "origins:or",
					"conditions": [
						{
							"type": "origins:power_active",
							"power": "*:fox/thweak_heavy_armor",
							"inverted": true
						},
						{
							"type": "origins:power",
							"power": "*:fox/thweak_heavy_armor",
							"inverted": true
						}
					]
				}
			]
		}
	},

	"stop_fall": {
		"type": "origins:modify_velocity",
		"condition": {
			"type": "origins:and",
			"conditions": [
				{
					"type": "origins:block_collision",
					"block_condition": {
						"type": "origins:block",
						"block": "minecraft:powder_snow"
					},
					"offset_y": -0.3
				},
				{
					"type": "origins:block_collision",
					"inverted": true,
					"block_condition": {
						"type": "origins:block",
						"block": "minecraft:powder_snow"
					},
					"offset_y": 0.3
				},
				{
					"type": "origins:resource",
					"resource": "*:*_jumping",
					"compare_to": 0,
					"comparison": "=="
				},
				{
					"type": "origins:resource",
					"resource": "*:*_sink_in",
					"compare_to": 0,
					"comparison": "=="
				},
				{
					"type": "origins:equipped_item",
					"inverted": true,
					"equipment_slot": "feet",
					"item_condition": {
						"type": "origins:ingredient",
						"ingredient": {
							"item": "minecraft:leather_boots"
						}
					}
				},
				{
					"type": "origins:or",
					"conditions": [
						{
							"type": "origins:power_active",
							"power": "*:fox/thweak_heavy_armor",
							"inverted": true
						},
						{
							"type": "origins:power",
							"power": "*:fox/thweak_heavy_armor",
							"inverted": true
						}
					]
				}
			]
		},
		"axes": ["y"],
		"modifier": {
			"value": 0,
			"operation": "set_total"
		}
	},

	"stop_slow_down": {
		"type": "origins:phasing",
		"condition": {
			"type": "origins:and",
			"conditions": [
				{
					"type": "origins:equipped_item",
					"inverted": true,
					"equipment_slot": "feet",
					"item_condition": {
						"type": "origins:ingredient",
						"ingredient": {
							"item": "minecraft:leather_boots"
						}
					}
				},
				{
					"type": "origins:in_block",
					"inverted": true,
					"block_condition": {
						"type": "origins:offset",
						"y": 1,
						"condition": {
							"type": "origins:block",
							"block": "minecraft:powder_snow"
						}
					}
				},
				{
					"type": "origins:block_collision",
					"block_condition": {
						"type": "origins:block",
						"block": "minecraft:powder_snow"
					}
				}
			]
		},
		"block_condition": {
			"type": "origins:block",
			"block": "minecraft:powder_snow"
		}
	},

	"stop_thrust_fall": {
		"type": "origins:modify_falling",
		"velocity": 0,
		"take_fall_damage": false,
		"condition": {
			"type": "origins:power_active",
			"power": "*:*_stop_fall"
		}
	}
}
